{
  "name": "jobscraper",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "test": "clear; ENVIRONMENT=test jest --runInBand ",
    "test-coverage": "clear; ENVIRONMENT=test jest --coverage",
    "migrate": "chmod +x ./scripts/run_migrations.sh; ./scripts/run_migrations.sh",
    "prettydev": "npm run build; clear; node --enable-source-maps dist/main.js; npm run format",
    "format": "prettier --log-level warn --write .; echo 'Succesfully formatted files.'",
    "remove-volumes": "sudo docker volume rm $(sudo docker volume ls -q)",
    "remove-containers": "sudo docker rm $(sudo docker ps -a -q)",
    "dev": "ENVIRONMENT=dev nodemon src/main.ts",
    "prod": "npm run build; ENVIRONMENT=prod node dist/src/main.js"
  },
  "author": "Mateo Nar",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "linkedom": "^0.15.4",
    "mocha": "^10.2.0",
    "nodemailer": "^6.9.5",
    "object-hash": "^3.0.0",
    "playwright": "^1.39.0",
    "postgres": "^3.3.5",
    "redis": "^4.6.10",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.2",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.2",
    "@types/mocha": "^10.0.1",
    "@types/nodemailer": "^6.4.10",
    "@types/object-hash": "^3.0.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1"
  }
}
